            const pd = new src_app_app_models__WEBPACK_IMPORTED_MODULE_1__["PollDesc"]();
            pd.language = flag.code;
            pd.title = '';
            pd.description = '';
            pd.endNote = '';
            this.poll.pollDescs.push(pd);
        }
