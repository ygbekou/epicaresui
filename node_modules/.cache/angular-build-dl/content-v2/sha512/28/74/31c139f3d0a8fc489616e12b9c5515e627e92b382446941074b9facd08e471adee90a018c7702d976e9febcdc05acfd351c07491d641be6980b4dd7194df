let StatusTextComponent = class StatusTextComponent extends src_app_shared_baseComponent__WEBPACK_IMPORTED_MODULE_5__["BaseComponent"] {
    constructor(appService, appSettings, translate, tokenStorage) {
        super(translate, tokenStorage);
        this.appService = appService;
        this.appSettings = appSettings;
        this.translate = translate;
        this.tokenStorage = tokenStorage;
        this.statusText = new src_app_app_models__WEBPACK_IMPORTED_MODULE_2__["StatusText"]();
    }
    ngOnInit() {
        this.flag = this.appService.getFlag();
        this.statusText.language = this.flag.code;
        this.getStatusText(this.flag);
    }
    getStatusText(flag) {
        this.messages = '';
        this.statusText = new src_app_app_models__WEBPACK_IMPORTED_MODULE_2__["StatusText"]();
        this.flag = flag ? flag : this.appService.getFlag();
        this.statusText.language = this.flag.code;
        console.log('Flag is set ...');
        const parameters = [];
        this.appService.getAllByCriteriaWithFiles('StatusText', parameters).subscribe((data) => {
            data.forEach((statusText) => {
                if (this.flag.code === statusText.language) {
                    this.statusText = statusText;
                }
            });
        });
    }
    save() {
        this.statusText.modifiedBy = +this.tokenStorage.getUserId();
        this.appService.save(this.statusText, 'StatusText')
            .subscribe(result => {
            this.processResult(result, this.statusText, undefined);
        });
    }
};
