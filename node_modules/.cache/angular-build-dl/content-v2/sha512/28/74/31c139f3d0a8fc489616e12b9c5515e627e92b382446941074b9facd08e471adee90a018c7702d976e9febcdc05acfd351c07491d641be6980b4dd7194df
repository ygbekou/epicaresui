let CmessageListComponent = class CmessageListComponent extends src_app_shared_baseComponent__WEBPACK_IMPORTED_MODULE_7__["BaseComponent"] {
    constructor(appService, translate, tokenStorage) {
        super(translate, tokenStorage);
        this.appService = appService;
        this.translate = translate;
        this.tokenStorage = tokenStorage;
        this.displayedColumns = ['id', 'name', 'email', 'phone', 'actions'];
    }
    ngOnInit() {
        const parameters = [];
        this.appService.getAllByCriteriaWithFiles('ContactUsMessage', parameters, ' ORDER BY e.id DESC ')
            .subscribe((data) => {
            this.dataSource = new _angular_material_table__WEBPACK_IMPORTED_MODULE_4__["MatTableDataSource"](data);
            this.dataSource.paginator = this.paginator;
            this.dataSource.sort = this.sort;
        });
    }
    remove(contactUsMessage) {
        const index = this.dataSource.data.indexOf(contactUsMessage);
        if (index !== -1) {
            this.appService.delete(contactUsMessage.id, 'ContactUsMessage')
                .subscribe(data => {
                this.processDeleteResult(data, this.messages);
                this.dataSource.data.splice(index, 1);
                this.dataSource = new _angular_material_table__WEBPACK_IMPORTED_MODULE_4__["MatTableDataSource"](this.dataSource.data);
                this.dataSource.paginator = this.paginator;
                this.dataSource.sort = this.sort;
            });
        }
    }
    applyFilter(filterValue) {
        this.dataSource.filter = filterValue.trim().toLowerCase();
        if (this.dataSource.paginator) {
            this.dataSource.paginator.firstPage();
        }
    }
};
