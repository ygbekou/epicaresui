let SlidersComponent = class SlidersComponent {
    constructor(appService, translate) {
        this.appService = appService;
        this.translate = translate;
        this.displayedColumns = ['id', 'rank', 'image', 'name', 'actions'];
        this.messages = '';
    }
    ngOnInit() {
        this.getAll();
    }
    getAll() {
        const parameters = [];
        this.appService.getAllByCriteria('com.wack.model.website.Slider', parameters)
            .subscribe((data) => {
            this.dataSource = new _angular_material_table__WEBPACK_IMPORTED_MODULE_4__["MatTableDataSource"](data);
            this.dataSource.paginator = this.paginator;
            this.dataSource.sort = this.sort;
        }, error => console.log(error), () => console.log('Get all Slider complete'));
    }
    applyFilter(filterValue) {
        this.dataSource.filter = filterValue.trim().toLowerCase();
        if (this.dataSource.paginator) {
            this.dataSource.paginator.firstPage();
        }
    }
};
