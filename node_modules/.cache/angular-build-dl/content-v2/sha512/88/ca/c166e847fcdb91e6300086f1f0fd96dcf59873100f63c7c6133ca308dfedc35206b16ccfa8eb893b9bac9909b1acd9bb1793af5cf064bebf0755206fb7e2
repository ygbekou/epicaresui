            rate: null,
            newsId: this.newsId,
            projectId: this.projectId
          });
          this.getComments(this.newsId, this.projectId);
          this.getNews(this.newsId, this.projectId);
        }
      }, {
        key: "getComments",
        value: function getComments(newsId, projectId) {
          var _this21 = this;

          var parameters = [];

          if (newsId > 0) {
            parameters.push('e.news.id = |newsId|' + newsId + '|Long');
          } else if (projectId > 0) {
            parameters.push('e.project.id = |pid|' + projectId + '|Long');
          }

          if (parameters.length > 0) {
            this.appService.getAllByCriteria('com.wack.model.website.Comment', parameters).subscribe(function (data) {
              _this21.reviews = data;
            }, function (error) {
              return console.log(error);
            }, function () {
              return console.log('Get all Comments complete');
            });
          }
        }
      }, {
        key: "getNews",
        value: function getNews(newsId, projectId) {
          var _this22 = this;

          console.log('Getting news:' + newsId);

          if (newsId > 0) {
            this.appService.getOne(newsId, 'com.wack.model.News').subscribe(function (result) {
              if (result.id > 0) {
                _this22.news = result;
              }
            });
          } else if (projectId > 0) {
            this.appService.getOne(projectId, 'com.wack.model.Project').subscribe(function (result) {
              if (result.id > 0) {
                _this22.project = result;
              }
            });
          }
        }
      }, {
        key: "onCommentFormSubmit",
        value: function onCommentFormSubmit(values) {
          var _this23 = this;

          if (this.commentForm.valid) {
            console.log(values);
            this.messages = null;
            var review = new src_app_app_models__WEBPACK_IMPORTED_MODULE_4__["Comment"]();
            review.author = values.name;
            review.authorEmail = values.email;
            review.message = values.review;
            review.rating = values.rate;

            if (this.news && this.news.id > 0) {
              review.news = this.news;
            }

            if (this.project && this.project.id > 0) {
              review.project = this.project;
            }

            review.modifiedBy = 0;
            this.appService.save(review, 'Comment').subscribe(function (data) {
              _this23.processResult(data, review, null);

              if (data.errors === null || data.errors.length === 0) {
                _this23.reviews.push(data);

                _this23.processResult(data, review, null);

                if (data.errors === null || data.errors.length === 0) {
                  _this23.commentForm.reset();
                }
              }
            });
          }
        }
      }, {
        key: "rate",
        value: function rate(rating) {
          var _this24 = this;

          this.ratings.filter(function (r) {
            return r.selected = false;
          });
          this.ratings.filter(function (r) {
            return r.percentage === rating.percentage;
          })[0].selected = true;
          this.commentForm.controls.rate.setValue(rating.percentage);

          if (this.newsId > 0) {
            this.news.ratingCount++;
            this.news.rating = this.news.rating + this.commentForm.controls.rate.value;
            this.appService.save(this.news, 'News').subscribe(function (data2) {
              // nothing to do
              console.log('Rating saved : rating= ' + _this24.news.rating + ', Rate count=' + _this24.news.ratingCount);
            });
          } else if (this.projectId > 0) {
            this.project.ratingCount++;
            this.project.rating = this.project.rating + this.commentForm.controls.rate.value;
            this.appService.save(this.project, 'Project').subscribe(function (data2) {
              // nothing to do
              console.log('Rating saved : rating= ' + _this24.project.rating + ', Rate count=' + _this24.project.ratingCount);
            });
          }
        }
      }]);

