          window.sessionStorage.removeItem(TokenStorage_1.ROLE_NAME);
          window.sessionStorage.removeItem(TokenStorage_1.PICTURE);
          window.sessionStorage.removeItem(TokenStorage_1.FIRST_NAME);
          window.sessionStorage.removeItem(TokenStorage_1.MIDDLE_NAME);
          window.sessionStorage.removeItem(TokenStorage_1.LAST_NAME);
          window.sessionStorage.removeItem(TokenStorage_1.FIRST_TIME_LOGIN);
          window.sessionStorage.removeItem(TokenStorage_1.USER_ID);
          window.sessionStorage.removeItem(TokenStorage_1.USER_NAME);
          window.sessionStorage.removeItem(TokenStorage_1.HOME_PAGE);
        }
      }, {
