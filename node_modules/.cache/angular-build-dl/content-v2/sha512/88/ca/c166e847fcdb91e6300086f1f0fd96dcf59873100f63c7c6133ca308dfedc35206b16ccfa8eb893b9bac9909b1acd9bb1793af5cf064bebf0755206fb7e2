        key: "saveAuthData",
        value: function saveAuthData(authData) {
          window.sessionStorage.removeItem(TokenStorage_1.TOKEN_KEY);
          window.sessionStorage.setItem(TokenStorage_1.TOKEN_KEY, authData.token); // user data

          window.sessionStorage.removeItem(TokenStorage_1.USER_GROUP_ID);
          window.sessionStorage.setItem(TokenStorage_1.USER_GROUP_ID, authData.authorities[0] + '');
          window.sessionStorage.removeItem(TokenStorage_1.ROLE_NAME);
          window.sessionStorage.setItem(TokenStorage_1.ROLE_NAME, authData.roleName);
          window.sessionStorage.removeItem(TokenStorage_1.PICTURE);
          window.sessionStorage.setItem(TokenStorage_1.PICTURE, !authData.picture || authData.picture === 'null' ? '' : authData.picture);
