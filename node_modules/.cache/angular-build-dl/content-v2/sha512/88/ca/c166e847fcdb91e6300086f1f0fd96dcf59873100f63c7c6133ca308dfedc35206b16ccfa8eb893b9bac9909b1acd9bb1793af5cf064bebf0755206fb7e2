      }, {
        key: "addToCompare",
        value: function addToCompare(news, component, direction) {
          if (!this.Data.compareList.filter(function (item) {
            return item.id === news.id;
          })[0]) {
            this.Data.compareList.push(news);
            this.bottomSheet.open(component, {
              direction: direction
            }).afterDismissed().subscribe(function (isRedirect) {
              if (isRedirect) {
                window.scrollTo(0, 0);
              }
            });
          }
        }
      }, {
        key: "addToFavorites",
        value: function addToFavorites(news, direction) {
          if (!this.Data.favorites.filter(function (item) {
            return item.id === news.id;
          })[0]) {
            this.Data.favorites.push(news);
            this.snackBar.open('The news "' + news.title + '" has been added to favorites.', '�', {
              verticalPosition: 'top',
              duration: 3000,
              direction: direction
            });
          }
        }
