        key: "changeCompany",
        value: function changeCompany(lang) {
          var _this4 = this;

          if (this.companies.length > 0) {
            this.companies.forEach(function (aCompany) {
              if (lang === aCompany.language) {
                _this4.company = aCompany;
                _this4.appSettings.settings = new _app_settings__WEBPACK_IMPORTED_MODULE_6__["Settings"]('Bat & Bel', // theme name
                _this4.company.themeColor, // blue, green, red, pink, purple, grey, orange-dark
                _this4.company.headerTextPosition, // 0, 1 or 2
                _this4.company.fixedMenu === 1 ? true : false, // true = sticky, false = not sticky
                _this4.company.headerImageType, // default, image, carousel
                _this4.company.leftToRight === 1 ? true : false, // true = rtl, false = ltr
                1, //  1, 2  or 3
                false, //  true = search on button click
                'USD', // USD, EUR
                // NOTE:  don't change additional options values, they used for theme performance
                false, false, false, {
                  start: false,
                  step: 1,
                  load: false,
                  page: 1,
                  complete: false,
                  result: 0
                });
                console.log('loging settings');
                console.log(_this4.appSettings.settings);
              }
            });
          }
        }
