          window.sessionStorage.setItem(TokenStorage_1.LAST_NAME, authData.lastName);
          window.sessionStorage.removeItem(TokenStorage_1.FIRST_TIME_LOGIN);
          window.sessionStorage.setItem(TokenStorage_1.FIRST_TIME_LOGIN, authData.firstTimeLogin);
          window.sessionStorage.removeItem(TokenStorage_1.USER_ID);
          window.sessionStorage.setItem(TokenStorage_1.USER_ID, authData.userId + '');
          window.sessionStorage.removeItem(TokenStorage_1.USER_NAME);
          window.sessionStorage.setItem(TokenStorage_1.USER_NAME, authData.userName + '');
          window.sessionStorage.removeItem(TokenStorage_1.HOME_PAGE);
          window.sessionStorage.setItem(TokenStorage_1.HOME_PAGE, authData.homePage);
        }
