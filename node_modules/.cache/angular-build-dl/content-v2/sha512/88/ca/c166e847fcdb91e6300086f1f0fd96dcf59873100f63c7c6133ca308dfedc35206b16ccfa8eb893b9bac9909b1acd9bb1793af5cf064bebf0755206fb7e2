      }, {
        key: "filterData",
        value: function filterData(data, params, sort, page, perPage) {
          this.sortData(sort, data);
          return this.paginator(data, page, perPage);
        }
      }, {
        key: "sortData",
        value: function sortData(sort, data) {
          if (sort) {
            console.log('Sort called: ' + sort);
            console.log(data);

            switch (sort) {
              case 'publicationAsc':
                data = data.sort(function (a, b) {
                  return new Date(b.publicationDatetime) - new Date(a.publicationDatetime);
                });
                console.log('Which one? publicationAsc');
                break;

              case 'publicationDesc':
                data = data.sort(function (a, b) {
                  return new Date(a.publicationDatetime) - new Date(b.publicationDatetime);
                });
                console.log('Which one? publicationDesc');
                break;

              case 'rating':
                data = data.sort(function (a, b) {
                  if (a.rating / a.ratingCount < b.rating / b.ratingCount) {
                    return 1;
                  }

                  if (a.rating / a.ratingCount > b.rating / b.ratingCount) {
                    return -1;
                  }

                  return 0;
                });
                console.log('Which one? rating');
                break;

              case 'vews':
                data = data.sort(function (a, b) {
                  if (a.viewCount < b.viewCount) {
                    return 1;
                  }

                  if (a.viewCount > b.viewCount) {
                    return -1;
                  }

                  return 0;
                });
                console.log('Which one? views');
                break;

              default:
                break;
            }
          }

          console.log(data);
          return data;
        }
      }, {
        key: "paginator",
        value: function paginator(items, page, perPage) {
          var page1 = page || 1,
              perPage1 = perPage || 4,
              offset = (page1 - 1) * perPage1,
              paginatedItems = items.slice(offset).slice(0, perPage1),
              totalPages = Math.ceil(items.length / perPage1);
          return {
            data: paginatedItems,
            pagination: {
              page1: page1,
              perPage1: perPage1,
              prePage: page1 - 1 ? page1 - 1 : null,
              nextPage: totalPages > page1 ? page1 + 1 : null,
              total: items.length,
              totalPages: totalPages
            }
          };
        }
