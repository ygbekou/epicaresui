              //orderComplete(result.paymentIntent.id);
              translate.get(['MESSAGE.CARD_PAYMENT_SUCCEDED']).subscribe(function (res) {
                myResult.messages = res['MESSAGE.CARD_PAYMENT_SUCCEDED'];
